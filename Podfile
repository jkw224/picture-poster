# platform :ios, '9.0'
use_frameworks!

target 'picture-poster' do

pod 'Firebase', '>= 2.4.0'

# Facebook 4.6.0 SDK -- need following 3 pods
# https://github.com/facebook/facebook-ios-sdk
pod 'FBSDKCoreKit', '~> 4.6'
pod 'FBSDKLoginKit', '~> 4.6'
pod 'FBSDKShareKit', '~> 4.6'

end